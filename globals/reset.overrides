/*******************************
         Theme Overrides
*******************************/

figure,
blockquote {
    margin: 0;
}

button::-moz-focus-inner {
    border: 0;
}

fieldset {
    border: 0;
    margin: 0;
    padding: 0.01em 0 0 0;
    min-width: 0;
}
legend {
    display: table;
    float: left;
    width: 100%;
    padding: 0;
}
body:not(:-moz-handler-blocked) fieldset {
    display: table-cell;
}